<IfModule mod_rewrite.c>
  RewriteEngine on
  RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization},L]
  RewriteBase "/"
  RewriteRule "^naf/api(.*)" "/index.php?req=$1" [L,PT,QSA]
</IfModule>

