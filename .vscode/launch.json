{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch built-in server and debug",
      "type": "php",
      "request": "launch",
      "runtimeArgs": [
        "-S",
        "127.0.0.1:8000/naf/api",
        "-t",
        "."
      ],
      "port": 9003,
      // "serverReadyAction": {
      //   "action": "openExternally"
      // }
    },
    {
      "name": "Debug current script in console",
      "type": "php",
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "externalConsole": false,
      "port": 9003
    },
    {
      "name": "Listen for Xdebug",
      "type": "php",
      "request": "launch",
      "port": 9003
    },
    {
      "name": "Debug current Script in Console",
      "detail": "Runs the current PHP script with debugging",
      "type": "php",
      "noDebug": false,
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "externalConsole": false
    }
  ]
}